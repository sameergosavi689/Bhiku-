if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())

    if x > y and x > z:
        if x > y:
            print(y)
        else:
            print(z)
    elif y > z:
        if y < x:
            print(y)
        else:
            print(z)
    elif z > x:
        if z > y:
            print(y)
        else:
            print(x)
else:
    print(z)
